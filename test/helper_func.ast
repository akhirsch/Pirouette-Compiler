compute_local x := (x, x);
main :=
  let
    A.(x, y) := compute_local A.(0);
    B.(x, y) := compute_local B.(1);
    C.(x, y) := compute_local C.(2);
  in
    let
      A.(x1, y1) := [B] B.((x, y)) ~> A;
      A.(x2, y2) := [C] C.((x, y)) ~> A;
    in
      let
        A.(sx, sy) := A.((x + x1 + x2, y + y1 + y2));
      in
        ();
